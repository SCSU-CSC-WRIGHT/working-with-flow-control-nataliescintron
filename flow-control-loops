#Lab 1 Guess the Number
import random

randnum = random.randint(1,10)
print(randnum)

attempts = 3
count = 0
while count < attempts:
    guess = int(input("Guess a number: "))
    if guess < randnum:
        print("Guess is too low")
    elif guess > randnum:
        print("Guess is too high")
    else: 
        print("YAYYYYY you win")
        break
    count += 1

if count ==  attempts:
    print('You ran out of attempts... You Lose')

#Lab 2 Countdown Timer

countdown = int(input("Enter a number to start coutndown: "))
end_num = 1

while countdown >= end_num:
    print(countdown)
    countdown -= 1

#Lab 3 Sum of Numbers

total = 0
user_num = int(input("Enter in a number: "))
for i in range(1, user_num +1):
    total+= i
print(total)

    



for letter in "Hello World":
    print(letter)


        



